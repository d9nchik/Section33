<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
      fx:controller="sample.exercise9.ChatServerController">
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Server"/>
    <ScrollPane prefHeight="200.0" prefWidth="200.0">
        <TextArea fx:id="serverText" onKeyPressed="#keyPressed" prefHeight="114.0" prefWidth="389.0" wrapText="true"/>
    </ScrollPane>
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Client"/>
    <ScrollPane prefHeight="200.0" prefWidth="200.0">
        <TextArea fx:id="clientText" editable="false" prefHeight="114.0" prefWidth="389.0" wrapText="true"/>
    </ScrollPane>
</VBox>
